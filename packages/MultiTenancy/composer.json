{
    "name": "encoda/multitenancy",
    "description": "Make app usable by multiple tenants, this package modified based on spatie/multitenancy",
    "keywords": [
        "encoda",
        "lumen-multitenancy"
    ],
    "homepage": "https://github.com/spatie/laravel-multitenancy",
    "license": "MIT",
    "authors": [
        {
            "name": "Spatie"
        },
        {
            "name": "Nam Nguyen",
            "email": "nam.nguyen@encoda.io",
            "homepage": "encoda.io",
            "role": "Technical Lead"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^8.0",
        "encoda/package-tools": "^1.9"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.0.4",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.23|^7.0",
        "phpunit/phpunit": "^9.4",
        "spatie/valuestore": "^1.2",
        "laravel/octane": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Encoda\\Multitenancy\\": "src",
            "Encoda\\Multitenancy\\Database\\Factories\\": "src/Database/Factories"

        }
    },
    "autoload-dev": {
        "psr-4": {
            "Encoda\\Multitenancy\\Tests\\": "tests"
        },
        "classmap": [
            "tests/database/migrations/landlord",
            "tests/database/migrations"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MultiTenancyServiceProvider"
            ],
            "aliases": {
                "Multitenancy": "Encoda\\MultiTenancy\\MultiTenancyFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
